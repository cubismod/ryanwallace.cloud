  "dependencies": {
    "@maptiler/leaflet-maptilersdk": "^4.0.2",
    "@petoc/leaflet-double-touch-drag-zoom": "^1.0.3",
    "@turf/turf": "^7.2.0",
    "@types/leaflet.markercluster": "^1.5.5",
    "datatables.net-dt": "^2.2.2",
    "datatables.net-responsive": "^3.0.4",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.5",
    "invert-color": "^2.0.0",
    "leaflet": "^1.9.4",
    "leaflet-arrowheads": "^1.4.0",
    "leaflet-easybutton": "^2.4.0",
    "leaflet.fullscreen": "^4.0.0",
    "leaflet.markercluster": "^1.5.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "string-comparison": "^1.3.0"
  },
  "prettier": "prettier-config-standard",
  "devDependencies": {
    "@types/dompurify": "3.2.0",
    "@types/leaflet": "1.9.20",
    "@types/node": "24.3.0",
    "buffer": "6.0.3",
    "parcel": "2.15.4",
    "prettier": "3.6.2",
    "prettier-config-standard": "7.0.0",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "parcel build src/index.html src/track.ts --public-url /map/",
    "build:nocache": "parcel build src/index.html src/track.ts --public-url /map/ --no-cache",
    "move": "mkdir -p ../static/map; mkdir -p ../content/map; mv dist/* ../content/map/",
    "title": "cat head.md ../content/map/index.html >  temp; mv temp ../content/map/index.html",
    "clean": "rm -f ../content/map/*; rm -f ../static/map/*",
    "format": "prettier --write \"src/**/*.{ts,js,json,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,html,css}\"",
    "typecheck": "tsc --noEmit"
  }
}
