  },
  "prettier": "prettier-config-standard",
  "sideEffects": [
    "*.css"
  ],
  "devDependencies": {
    "@types/jquery": "3.5.33",
    "@types/leaflet": "1.9.20",
    "@types/node": "24.3.3",
    "buffer": "6.0.3",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.2",
    "events": "3.3.0",
    "html-webpack-plugin": "5.6.4",
    "mini-css-extract-plugin": "2.9.4",
    "prettier": "3.6.2",
    "prettier-config-standard": "7.0.0",
    "style-loader": "4.0.0",
    "terser-webpack-plugin": "5.3.14",
    "ts-loader": "9.5.4",
    "typescript": "5.9.2",
    "webpack": "5.101.3",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-cli": "6.0.1"
  },
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:dev": "webpack --config webpack.config.js --mode development",
    "build:nocache": "webpack --config webpack.config.js --mode production --no-cache",
    "move": "mkdir -p ../static/map; mkdir -p ../content/map; mkdir -p ../content/alerts; mkdir -p ../content/track; if [ -f dist/alerts.html ]; then mv dist/alerts.html ../content/alerts/index.html; fi; if [ -f dist/track.html ]; then mv dist/track.html ../content/track/index.html; fi; mv dist/* ../content/map/",
    "title": "cat head.md ../content/map/index.html >  temp; mv temp ../content/map/index.html",
    "title:alerts": "cat head-alerts.md ../content/alerts/index.html > temp; mv temp ../content/alerts/index.html",
    "title:track": "cat head-track.md ../content/track/index.html > temp; mv temp ../content/track/index.html",
    "clean": "rm -f ../content/map/*; rm -f ../static/map/*",
    "format": "prettier --write \"src/**/*.{ts,js,json,html,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,html,css}\"",
    "typecheck": "tsc --noEmit",
    "analyze": "webpack --config webpack.config.js --profile --json=dist/stats.json && webpack-bundle-analyzer dist/stats.json"
  }
}
