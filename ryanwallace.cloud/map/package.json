    "@turf/length": "^7.2.0",
    "@turf/nearest-point-on-line": "^7.2.0",
    "@types/leaflet.markercluster": "^1.5.5",
    "datatables.net": "^2.3.3",
    "datatables.net-dt": "^2.3.3",
    "datatables.net-jqui": "^2.3.3",
    "datatables.net-responsive": "^3.0.4",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.0.0",
    "dompurify": "^3.2.5",
    "jquery": "^3.7.1",
    "leaflet": "^1.9.4",
    "leaflet-easybutton": "^2.4.0",
    "leaflet.fullscreen": "^4.0.0",
